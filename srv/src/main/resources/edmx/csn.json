    "localized.sap.common.Countries": {
      "kind": "entity",
      "@UI.Identification": [
        {
          "Value": {
            "=": "name"
          }
        }
      ],
      "@cds.autoexpose": true,
      "@cds.odata.valuelist": true,
      "@cds.persistence.skip": "if-unused",
      "@odata.draft.enabled": false,
      "query": {
        "SELECT": {
          "from": {
            "ref": [
              "sap.common.Countries"
            ],
            "as": "L"
          },
          "columns": [
            "*",
            {
              "func": "coalesce",
              "args": [
                {
                  "ref": [
                    "L",
                    "localized",
                    "name"
                  ]
                },
                {
                  "ref": [
                    "L",
                    "name"
                  ]
                }
              ],
              "as": "name"
            },
            {
              "func": "coalesce",
              "args": [
                {
                  "ref": [
                    "L",
                    "localized",
                    "descr"
                  ]
                },
                {
                  "ref": [
                    "L",
                    "descr"
                  ]
                }
              ],
              "as": "descr"
            }
          ]
        }
      },
      "elements": {
        "name": {
          "@Core.Computed": true,
          "localized": true,
          "type": "cds.String",
          "length": 255,
          "@Common.Label": "{i18n>Name}"
        },
        "descr": {
          "@Core.Computed": true,
          "localized": true,
          "type": "cds.String",
          "length": 1000,
          "@Common.Label": "{i18n>Description}"
        },
        "code": {
          "@Common.Text": {
            "=": "name"
          },
          "key": true,
          "type": "cds.String",
          "length": 3,
          "@Common.Label": "{i18n>CountryCode}"
        },
        "texts": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "sap.common.Countries_texts",
          "on": [
            {
              "ref": [
                "texts",
                "code"
              ]
            },
            "=",
            {
              "ref": [
                "code"
              ]
            }
          ]
        },
        "localized": {
          "type": "cds.Association",
          "target": "sap.common.Countries_texts",
          "on": [
            {
              "ref": [
                "localized",
                "code"
              ]
            },
            "=",
            {
              "ref": [
                "code"
              ]
            },
            "and",
            {
              "ref": [
                "localized",
                "locale"
              ]
            },
            "=",
            {
              "ref": [
                "$user",
                "locale"
              ]
            }
          ]
        }
      }
    },
